{
  "name": "movie-management-api",
  "version": "1.0.0",
  "description": "API para gestión de películas con Node.js, Express",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/src/index.js",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "postinstall": "npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "engines": {
    "node": "20.x"
  },
  "keywords": [
    "nodejs",
    "express",
    "prisma",
    "Noen",
    "typescript"
  ],
  "author": "@Uber830",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.17",
    "morgan": "^1.10.1",
    "prisma": "^5.7.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
}
